{
  "name": "@entro314labs/entro-react",
  "version": "1.0.0",
  "description": "React SDK for Entrolytics - First-party growth analytics for the edge",
  "keywords": [
    "entrolytics",
    "analytics",
    "react",
    "web-analytics",
    "tracking",
    "spa"
  ],
  "author": "Entro314 Labs <hey@entrolytics.click>",
  "license": "MIT",
  "homepage": "https://entrolytics.click",
  "repository": {
    "type": "git",
    "url": "https://github.com/entro314-labs/entro-react",
    "directory": "entro-react"
  },
  "bugs": {
    "url": "https://github.com/entro314labs/entro-react/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=22.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
